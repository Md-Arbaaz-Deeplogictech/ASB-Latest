{
	"flow":{
		"name":"XML to JSON",
		"disabled":true
	},
	"listener": {
		"type":"file", 
		"isMessageGenerator": true,
		"path":"c:/test/in/Test.xml",
		"donePath":"c:/test/processing"
	},
	"route0":{
		"type": "filereader",
		"dependencies":["listener"],
        "donePath":"c:/test/done",
        "encoding":"utf8"
	},
	"route1": {
		"type":"xmlparser",
		"dependencies":["route0"]
	},
	"output": {
		"type": "filewriter",
        "dependencies":"route1",
		"path":"c:/test/Test.json",
		"prettyJSON": 4,
		"append":false
	}
}