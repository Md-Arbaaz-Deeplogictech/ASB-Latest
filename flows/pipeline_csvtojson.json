{
	"flow":{
		"name":"CSV to JSON",
		"disabled":true
	},
	"listener": {
		"type":"file", 
		"isMessageGenerator": true,
		"path":"c:/test/in/*.csv",
		"donePath":"c:/test/processing"
	},
	"route0":{
		"type": "csvfilereader",
		"dependencies":["listener"],
		"rowsPerParse": 10000,
		"skip_first_row":true,
        "donePath":"c:/test/done"
	},
	"route1": {
		"type":"csvparser",
		"dependencies":["route0","!listener"],
		"csv_headers":["Date","Open","High","Low","Close","Adj Close","Volume"],
		"trimValues": true
	},
	"output": {
		"type": "filewriter",
        "dependencies":"route1",
		"path":"c:/test/VOO.ndjson",
		"write_ndjson": true,
		"append": true,
		"writeCloseTimeout": 5000,
		"encoding": "utf8"
	}
}