{
	"flow":{
		"name":"CSV to JSON",
		"disabled":false
	},
	"listener": {
		"type":"file", 
		"isMessageGenerator": true,
		"path":"c:/test/in/^GSPC.csv",
		"donePath":"c:/test/processing"
	},
	"route0":{
		"type": "csvfilereader",
		"dependencies":["listener"],
		"rowsPerParse": 1000,
		"skip_first_row":true,
        "donePath":"c:/test/done"
	},
	"route1": {
		"type":"csvparser",
		"dependencies":["route0","!listener"],
		"csv_headers":["Date","Open","High","Low","Close","Adj Close","Volume"],
		"trimValues": true
	},
	"output": {
		"type": "filewriter",
        "dependencies":"route1",
		"path":"c:/test/^GSPC.json",
		"prettyJSON": 4,
		"write_ndjson": true,
		"append":true
	}
}